import xlrd,xlwt,xlutils
from cod_utf8 import cod_utf8
import sys
reload(sys)
sys.setdefaultencoding('utf-8')
def xl_red(row_num):
    exl=xlrd.open_workbook(u"C:/test/更新日志.xls")
    ex_sheet=exl.sheet_by_name(u"2016年11月份")
    
    cell=ex_sheet.row_values(row_num+2)
    item=["更新代号：\n","更新功能：\n","文件列表：\n","SVN版本号：\n","开发人员：\n"]
    num=0
    textlist=[]
    for stri in item:
        textlist.append(stri+"******"+str(cell[num]))
        num+=1
    return textlist  
    
    
if __name__=="__main__":
    textlist=xl_red(2)
    print textlist
    